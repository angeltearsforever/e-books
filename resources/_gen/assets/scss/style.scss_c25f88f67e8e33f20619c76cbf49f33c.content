.qzine-page{display:flex;align-items:center;justify-content:center;flex-direction:column;margin:15px auto;width:290px}.qzine-page h1,.qzine-page h2,.qzine-page h3,.qzine-page h4,.qzine-page h5,.qzine-page h6,.qzine-page p{font-family:"Open Sans" !important;color:#fff;line-height:1.4em !important;text-align:center;letter-spacing:.05rem}.qzine-page h1{font-size:36px}.qzine-page p{font-size:20px;margin:0 !important;font-weight:500}@media screen and (max-width: 500px){.qzine-page p{font-size:16px}}.qzine-page:first-child{min-height:60vh;margin-bottom:45px}@media screen and (min-width: 500px){.qzine-page:first-child{min-height:40vh}}@media screen and (min-width: 600px){.qzine-page{width:500px}}@media screen and (min-width: 1200px){.qzine-page{width:1000px}}.qzine-page img{margin-bottom:15px !important;padding:0;margin-top:0}@media screen and (max-width: 500px){.qzine-page img{height:206px}}@media screen and (min-width: 500px){.qzine-page img{height:295px}}@media screen and (min-width: 1000px){.qzine-page img{height:500px}}@media screen and (max-width: 500px){.video-embed p,.images-with-text p{font-size:9px !important}}.video-embed{background-color:black;text-align:center;margin-bottom:15px;box-shadow:0px 0px 16px 0px rgba(255,255,255,0.75) !important;color:white !important;width:calc(100vw - (5vw * 2));height:calc(61vw - (5vw * 2 * .61));overflow:hidden;width:290px;height:178px}@media screen and (min-width: 600px){.video-embed{width:500px;height:310px}}@media screen and (min-width: 1200px){.video-embed{width:1000px;height:610px}}.video-embed iframe{transform:translate(-32%, -24%);overflow:hidden;object-position:center;width:calc(195vw - (5vw * 2));height:calc(119vw - (5vw * 2 * .61))}@media screen and (min-width: 1028px){.video-embed iframe{width:calc(205vw - (13vw * 2));height:calc(125vw - (13vw * 2 * .61))}}.images-with-text{height:100%;width:100%;display:flex;justify-content:space-between}.images-with-text>*{flex:0 0 48%;display:flex;flex-wrap:wrap}.images-with-text>*>p{flex:1 1 100%;display:flex;justify-content:stretch;align-items:stretch;text-align:center}.images-with-text p{margin-bottom:20px;display:flex;justify-content:center}@media screen and (max-width: 500px){.images-with-text img{height:206px}}@media screen and (min-width: 500px){.images-with-text img{height:295px}}@media screen and (min-width: 1000px){.images-with-text img{height:500px}}.images-with-text img,.images-with-text a:hover img{object-fit:cover;box-shadow:0px 0px 16px 0px rgba(255,255,255,0.75) !important;cursor:default}.images-with-text .inline.popup,.images-with-text .inline.popup:hover{color:white !important;border-bottom:1px solid white !important}.quire,.quire__primary{height:100%;width:100%}.quire-splash{height:100%;width:100%}.cover__hero__bottom .video-embed{margin-bottom:0 !important;width:100% !important}.cover__hero__bottom .video-embed iframe{transform:unset;margin:0 auto;width:100vw;height:61vw}.cover__hero__bottom img{background-repeat:no-repeat;object-fit:cover;width:100%;margin-bottom:-10px}.material-fullscreen{display:none}.blue-moon-embed{margin:0 auto;padding-bottom:50px;text-align:center}.quire-page__content{background-color:green}html,body{max-width:100vw;max-height:100vh;background-color:green}.butterfly:first-child{--random-x: 100px;--random-y: 200px}.butterfly:last-child{--random-x: 200px;--random-y: 400px}.butterfly-wrapper{position:absolute;width:100vw;height:200vh}.canvas{position:fixed;width:100vw;height:100vh;margin:0 auto}.butterfly-body{width:4px;height:25px;border-radius:10px;background-color:rgba(255,255,255,0.5)}.butterfly-body:before{position:absolute;top:-5px;left:-1px;width:1px;height:6px;background-color:rgba(255,255,255,0.5);transform:rotate(-25deg);content:''}.butterfly-body:after{position:absolute;top:-5px;left:4px;width:1px;height:6px;background-color:rgba(255,255,255,0.5);transform:rotate(25deg);content:''}.butterfly{position:absolute;top:0px;left:0px;transform:translate(var(--random-x), var(--random-y));transition:transform 5s linear}.butterfly-wing-1{transform:rotateY(60deg);animation:Wing1 1s infinite}.butterfly-wing-1:before{position:absolute;top:-9px;left:5px;display:block;width:30px;height:20px;transform:rotateX(0deg);border-radius:20px 5px;background-color:rgba(255,255,255,0.5);content:''}.butterfly-wing-1:after{position:absolute;top:13px;left:5px;display:block;width:30px;height:20px;transform:rotateX(-170deg);border-radius:20px 5px;background-color:rgba(255,255,255,0.5);content:''}.butterfly-wing-2{position:relative;top:-1px;transform:rotate(180deg) rotateY(60deg);animation:Wing2 1s infinite}.butterfly-wing-2:before{position:absolute;top:-9px;left:5px;display:block;width:30px;height:20px;transform:rotateX(0deg);border-radius:20px 5px;background-color:rgba(255,255,255,0.5);content:''}.butterfly-wing-2:after{position:absolute;top:13px;left:5px;display:block;width:30px;height:20px;transform:rotateX(-170deg);border-radius:20px 5px;background-color:rgba(255,255,255,0.5);content:''}@keyframes Wing1{0%{transform:rotateY(60deg)}50%{transform:rotateY(0deg)}100%{transform:rotateY(60deg)}}@keyframes Wing2{0%{transform:rotate(180deg) rotateY(60deg)}50%{transform:rotate(180deg) rotateY(0deg)}100%{transform:rotate(180deg) rotateY(60deg)}}@keyframes Body{0%{transform:translate(88, 113)}20%{transform:translate(186, 155)}40%{transform:translate(85, 93)}60%{transform:translate(74, 155)}80%{transform:translate(19, 98)}100%{transform:translate(167, 18)}}
